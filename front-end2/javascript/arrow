// Arrow function
const somar = (a,b) => {
    return a + b
}
 
const subtrair = (a,b) => {
    return a - b
}

const multiplicar = (a,b) => {
    return a * b

}
    
const dividir = (a,b) => {
    return a / b
}



// Chamando uma arrow function
const soma = somar(4,2)
const subtracao = subtrair(4,2)
const multiplicacao = multiplicar(4,2)
const divisao = dividir(4,2)

console.log(`Soma: ${soma}`)
console.log(`Subtracao: ${subtracao}`)
console.log(`Multiplicacao: ${multiplicacao}`)
console.log(`Soma: ${divisao}`)
